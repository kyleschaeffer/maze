{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "rules": {
    "arrow-parens": false,
    "curly": [true, "ignore-same-line"],
    "max-line-length": false,
    "object-literal-sort-keys": false,
    "one-line": false,
    "quotemark": [true, "single", "avoid-escape", "avoid-template"]
  }
}
